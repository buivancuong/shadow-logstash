{
	"settings":{
		"analysis":{
			"analyzer":{
				"default":{"type":"custom","tokenizer":"my_tokenizer","filter":["lowercase"]},
				"default_search":{"type":"custom","tokenizer":"keyword","filter":["lowercase"]}
			},
			"tokenizer":{
				"my_tokenizer":{"type":"ngram","min_gram":2,"max_gram":12}
			}
		},
		"max_ngram_diff":10
	},
	"mappings":{
		"dynamic": false,
		"properties":{
			"@timestamp":			{"type":"date"},
			"@version":				{"type":"keyword"},
			"message":				{"type":"keyword"},
			"tags":					{"type":"keyword"},
			"LogTime":				{"type":"date","format":"yyyy-MM-dd/HH:mm:ss"},

			"SrcIP":				{"type":"ip"},
			"SrcIP_CityName":		{"type":"keyword"},
            "SrcIP_RegionName":		{"type":"keyword"},
            
            "BluekeepVuln":         {"type":"keyword"},
            "CertExpirationDate":   {"type":"date","format":"yyyy-MM-dd/HH:mm:ss"},
            "CertIssueDate":        {"type":"date","format":"yyyy-MM-dd/HH:mm:ss"},
            "CertLength":           {"type":"keyword"},
            "CertSerialNumber":     {"type":"keyword"},
            "CVEVuln":              {"type":"keyword"},
            "HandShake":            {"type":"keyword"},
            "IssuerCommonName":     {"type":"keyword"},
            "KeyAlgorithm":         {"type":"keyword"},
            "MD5FingerPrint":       {"type":"keyword"},
            "Orgs":                 {"type":"keyword"},
            "RDPProtocol":          {"type":"keyword"},
            "Sector":               {"type":"keyword"},
            "SHA1FingerPrint":      {"type":"keyword"},
            "SHA256FingerPrint":    {"type":"keyword"},
            "SHA512FingerPrint":    {"type":"keyword"},
            "SignatureAlgorithm":   {"type":"keyword"},
            "SrcIP_ASN":            {"type":"keyword"},
            "SrcPort":              {"type":"keyword"},
            "SSLVersion":           {"type":"keyword"},
            "SubjectCommonName":    {"type":"keyword"},
            "Tag":                  {"type":"keyword"},
            "TLSv13Cipher":         {"type":"keyword"},
            "TLSv13Support":        {"type":"keyword"}
		}
	}
}
